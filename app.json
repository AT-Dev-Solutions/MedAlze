{
  "expo": {
    "name": "MedAlze AI",
    "slug": "medalze-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "medalze-ai",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.medalze.ai",
      "infoPlist": {
        "NSCameraUsageDescription": "This app requires camera access to capture X-ray images.",
        "NSPhotoLibraryUsageDescription": "This app requires photo library access to select X-ray images."
      }
    },
    "android": {
      "package": "com.medalze.ai",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-apple-authentication"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
